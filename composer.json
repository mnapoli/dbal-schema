{
    "name": "mnapoli/dbal-schema",
    "description": "DB schema manager for Doctrine DBAL",
    "keywords": ["doctrine", "dbal", "db", "schema"],
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "DbalSchema\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DbalSchema\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.0",
        "doctrine/dbal": "^2.11 || ^3.0",
        "symfony/console": "^3.0 || ^4.0 || ^5.0 || ^6.0 || ^7.0",
        "doctrine/migrations": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    }
}
